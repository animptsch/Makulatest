<UserControl x:Class="MakulaTest.AnalyseControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MakulaTest"
             mc:Ignorable="d"
             d:DesignHeight="400" d:DesignWidth="400"
             SizeChanged="MyCanvas_SizeChanged">

    <StackPanel Orientation="Vertical">
      <Canvas Visibility="Visible" Background="White" HorizontalAlignment="Left" x:Name="MyCanvas">

      </Canvas>

        <Button Content="Zur Hauptseite" Click="Button_Click"/>
    </StackPanel>




</UserControl>
